@model Survey

<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-gH2yIJqKdNHPEq0n4Mqa/HGKIhSkIHeL5AyhkYV8i59U5AR6csBvApHHNl/vI1Bx" crossorigin="anonymous">
    <title>Document</title>
</head>

<body>
    <div class="container bg-light p-3">

        <div class="border border-3 border-dark rounded p-5">

            <h1>Submitted Info</h1>

            <div class="p-5">

                <div class="row g-3 mb-4 align-items-center">
                    <h4 class="col-6">Name:</h4>
                    <h5 class="col-6">@Model.Name</h5>
                </div>

                <div class="row g-3 mb-4 align-items-center">
                    <h4 class="col-6">Dojo Location:</h4>
                    <h5 class="col-6">@Model.Location</h5>
                </div>

                <div class="row g-3 mb-4 align-items-center">
                    <h4 class="col-6">Language:</h4>
                    <h5 class="col-6">@Model.Language</h5>
                </div>

                <div class="row g-3 mb-4 align-items-center">
                    <h4 class="col-6">Comments:</h4>
                    <h5 class="col-6">@Model.Comment</h5>
                </div>

            </div>

            <div class="row justify-content-end">
                @* Submit Button *@
                <a href="/" class="col-2 me-2 btn btn-primary">Go Back</a>
            </div>

        </div>
    </div>


    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.0/dist/js/bootstrap.bundle.min.js"
        integrity="sha384-A3rJD856KowSb7dwlZdYEkO39Gagi7vIsF0jrRAoQmDKKtQBHUuLZ9AsSv4jD4Xa"
        crossorigin="anonymous"></script>
</body>

</html>